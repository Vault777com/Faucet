# Sepolia testnet RPC URL (from Infura, Alchemy, etc.)
SEPOLIA_URL=https://sepolia.infura.io/v3/YOUR_INFURA_KEY

# Private key of the account used for deployment (without 0x prefix)
PRIVATE_KEY=your_private_key_here

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Arbiscan API Key for Arbitrum contract verification (same as Etherscan key)
ARBISCAN_API_KEY=your_etherscan_api_key_here

# ERC20 token address to use for the faucet
TOKEN_ADDRESS=your_erc20_token_address_here

